{
  "cleanUrls": true,
  "rewrites": [
    {   "source": "/",        
        "destination": "/site/index.html" 
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|svg|gif|webp)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
}
